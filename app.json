
{
  "expo": {
    "name": "CyntientOps",
    "slug": "cyntientops",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/mobile-rn/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./apps/mobile-rn/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyntientops.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/mobile-rn/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./apps/mobile-rn/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "swiftVersion": "5.0"
          }
        }
      ]
    ]
  }
}
