# CyntientOps Mobile App - Environment Configuration

# Database
DATABASE_PATH=cyntientops.db

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_AI_ENABLED=true
SUPABASE_REALTIME_ENABLED=true
SUPABASE_ANALYTICS_ENABLED=true

# WebSocket Configuration
WEBSOCKET_URL=ws://localhost:8080/ws

# NYC Open Data API Keys (OPTIONAL)
# These APIs are PUBLIC - keys only needed for higher rate limits
# Without keys: ~1000 requests/day (shared pool)
# With keys: 1000 requests/hour per app
# Get free keys at: https://data.cityofnewyork.us/
DSNY_API_KEY=
HPD_API_KEY=
DOB_API_KEY=
DOB_SUBSCRIBER_KEY=

# Weather API
WEATHER_API_KEY=your_weather_api_key_here

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_REALTIME_SYNC=true
ENABLE_INTELLIGENCE=true
ENABLE_WEATHER_INTEGRATION=true

# Debug
DEBUG=false
LOG_LEVEL=info
