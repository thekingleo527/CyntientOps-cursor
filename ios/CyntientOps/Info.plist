    <array>
      <string>tel</string>
      <string>mailto</string>
      <string>maps</string>
      <string>comgooglemaps</string>
      <string>uber</string>
      <string>lyft</string>
    </array>
    <key>LSMinimumSystemVersion</key>
    <string>12.0</string>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>NSAppTransportSecurity</key>
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <false/>
      <key>NSAllowsLocalNetworking</key>
      <true/>
    </dict>
    <key>NSBluetoothAlwaysUsageDescription</key>
    <string>CyntientOps uses Bluetooth for device connectivity and proximity-based features.</string>
    <key>NSBluetoothPeripheralUsageDescription</key>
    <string>CyntientOps uses Bluetooth for device connectivity and proximity-based features.</string>
    <key>NSCameraUsageDescription</key>
    <string>CyntientOps needs camera access to capture photo evidence for task completion and compliance documentation.</string>
    <key>NSContactsUsageDescription</key>
    <string>CyntientOps needs contacts access to manage building contacts and emergency contacts.</string>
    <key>NSFaceIDUsageDescription</key>
    <string>CyntientOps uses Face ID for secure authentication and biometric login.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>CyntientOps needs location access to provide building-specific services and route optimization.</string>
    <key>NSLocationAlwaysUsageDescription</key>
    <string>Allow $(PRODUCT_NAME) to access your location</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>CyntientOps needs location access to provide building-specific services and route optimization.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>CyntientOps needs microphone access for Nova AI voice interactions and emergency communication.</string>
    <key>NSMotionUsageDescription</key>
    <string>CyntientOps uses motion sensors for Nova AI gesture recognition and device orientation.</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>CyntientOps needs permission to save photos to your photo library.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>CyntientOps needs photo library access to select and upload images for task documentation.</string>
    <key>NSTouchIDUsageDescription</key>
    <string>CyntientOps uses Touch ID for secure authentication and biometric login.</string>
    <key>NSUserActivityTypes</key>
    <array>
      <string>com.cyntientops.task</string>
      <string>com.cyntientops.building</string>
      <string>com.cyntientops.emergency</string>
    </array>
    <key>RCTNewArchEnabled</key>
    <true/>
    <key>UIBackgroundModes</key>
    <array>
      <string>background-fetch</string>
      <string>background-processing</string>
      <string>remote-notification</string>
      <string>location</string>
      <string>fetch</string>
    </array>
