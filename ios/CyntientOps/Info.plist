    <array>
      <dict>
        <key>CFBundleURLName</key>
        <string>cyntientops</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>cyntientops</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>exp+cyntientops-mobile</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>
