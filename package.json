{
  "name": "@cyntientops/cursor",
  "version": "1.0.0",
  "description": "CyntientOps TypeScript/React Native implementation with complete data parity",
  "private": true,
  "workspaces": [],
  "scripts": {
    "mobile:prewarm-expo": "cd apps/mobile-rn && npx -y expo@latest --version",
    "mobile:setup": "cd apps/mobile-rn && rm -rf node_modules && (npx -y expo@latest install --non-interactive || npm i)",
    "mobile:setup:force": "cd apps/mobile-rn && rm -rf node_modules package-lock.json && npm i --legacy-peer-deps",
    "mobile:doctor": "cd apps/mobile-rn && npx expo doctor",
    "mobile:clean": "cd apps/mobile-rn && rm -rf node_modules && rm -rf $TMPDIR/metro-cache && rm -rf $TMPDIR/react-* || true",
    "mobile:start": "cd apps/mobile-rn && npm run start",
    "mobile:ios": "cd apps/mobile-rn && npm run ios",
    "mobile:android": "cd apps/mobile-rn && npm run android",
    "mobile:reset": "cd apps/mobile-rn && npx expo start -c",
    "mobile:prebuild": "cd apps/mobile-rn && npx expo prebuild",
    "mobile:run:ios": "cd apps/mobile-rn && npx expo run:ios",
    "mobile:run:android": "cd apps/mobile-rn && npx expo run:android",
    "dev:mobile": "nx run mobile-rn:start",
    "dev:web": "nx run web-dashboard:dev",
    "validate:data": "nx run data-seed:validate",
    "build:all": "nx run-many --target=build",
    "build:design-tokens": "nx run design-tokens:build",
    "build:ui-components": "nx run ui-components:build",
    "build:database": "nx run database:build",
    "build:managers": "nx run managers:build",
    "build:intelligence": "nx run intelligence-services:build",
    "build:context": "nx run context-engines:build",
    "build:commands": "nx run command-chains:build",
    "build:offline": "nx run offline-support:build",
    "build:realtime": "nx run realtime-sync:build",
    "build:testing": "nx run testing:build",
    "test:all": "nx run-many --target=test",
    "test:e2e": "nx run testing:test:e2e",
    "lint:all": "nx run-many --target=lint",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "install:all": "npm install && npm run --workspaces install"
  },
  "devDependencies": {
    "@nx/js": "^18.0.0",
    "@nx/react-native": "^18.0.0",
    "@nx/workspace": "^18.0.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/eslint-config": "^0.81.1",
    "@types/geokdbush": "^1.1.5",
    "@types/react": "^19.1.15",
    "@types/react-native": "^0.72.8",
    "@types/supercluster": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-native": "^5.0.0",
    "nx": "^18.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyntientops/cursor.git"
  },
  "keywords": [
    "cyntientops",
    "field-operations",
    "building-maintenance",
    "react-native",
    "typescript",
    "nyc-apis"
  ],
  "author": "CyntientOps Team",
  "license": "MIT",
  "dependencies": {
    "geokdbush": "^2.0.1",
    "kdbush": "^4.0.2",
    "lib0": "^0.2.114",
    "supercluster": "^8.0.1",
    "y-indexeddb": "^9.0.12",
    "yjs": "^13.6.27",
    "brain.js": "^2.0.0-beta.24",
    "ml-regression": "^6.1.3",
    "ml-kmeans": "^6.0.0",
    "ml-matrix": "^6.11.1",
    "natural": "^6.10.2",
    "compromise": "^14.11.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
