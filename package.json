{
  "name": "@cyntientops/cursor",
  "version": "1.0.0",
  "description": "CyntientOps TypeScript/React Native implementation with complete data parity",
  "private": true,
  "workspaces": [],
  "scripts": {
    "dev:mobile": "nx run mobile-rn:start",
    "dev:web": "nx run web-dashboard:dev",
    "validate:data": "nx run data-seed:validate",
    "build:all": "nx run-many --target=build",
    "build:design-tokens": "nx run design-tokens:build",
    "build:ui-components": "nx run ui-components:build",
    "build:database": "nx run database:build",
    "build:managers": "nx run managers:build",
    "build:intelligence": "nx run intelligence-services:build",
    "build:context": "nx run context-engines:build",
    "build:commands": "nx run command-chains:build",
    "build:offline": "nx run offline-support:build",
    "build:realtime": "nx run realtime-sync:build",
    "build:testing": "nx run testing:build",
    "test:all": "nx run-many --target=test",
    "test:e2e": "nx run testing:test:e2e",
    "lint:all": "nx run-many --target=lint",
    "clean": "nx reset && rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "install:all": "npm install && npm run --workspaces install"
  },
  "devDependencies": {
    "@nx/js": "^18.0.0",
    "@nx/react-native": "^18.0.0",
    "@nx/workspace": "^18.0.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/eslint-config": "^0.81.1",
    "@types/react": "^19.1.15",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-native": "^5.0.0",
    "nx": "^18.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyntientops/cursor.git"
  },
  "keywords": [
    "cyntientops",
    "field-operations",
    "building-maintenance",
    "react-native",
    "typescript",
    "nyc-apis"
  ],
  "author": "CyntientOps Team",
  "license": "MIT"
}
