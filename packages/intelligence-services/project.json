{
  "name": "intelligence-services",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/intelligence-services/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/intelligence-services",
        "main": "packages/intelligence-services/src/index.ts",
        "tsConfig": "packages/intelligence-services/tsconfig.lib.json",
        "assets": [
          "packages/intelligence-services/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/intelligence-services/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/intelligence-services/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": [
    "scope:intelligence",
    "type:lib"
  ]
}